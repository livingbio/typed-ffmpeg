    },
    {
      "alias": [
        "lowpass"
      ],
      "choices": [
        {
          "flags": "..FV.......",
          "help": "disable vertical low-pass filter",
          "name": "off",
          "value": "0"
        },
        {
          "flags": "..FV.......",
          "help": "linear vertical low-pass filter",
          "name": "linear",
          "value": "1"
        },
        {
          "flags": "..FV.......",
          "help": "complex vertical low-pass filter",
          "name": "complex",
          "value": "2"
        }
      ],
      "default": "linear",
      "description": "set vertical low-pass filter (from 0 to 2) (default linear)",
      "flags": "..FV.......",
      "max": "2",
      "min": "0",
      "name": "lowpass",
      "typing": "int"
