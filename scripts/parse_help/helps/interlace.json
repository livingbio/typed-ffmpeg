  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "interlace",
  "options": [
    {
      "alias": [
        "scan"
      ],
      "choices": [
        {
          "flags": "..FV.......",
          "help": "top field first",
          "name": "tff",
          "value": "0"
        },
        {
          "flags": "..FV.......",
          "help": "bottom field first",
          "name": "bff",
          "value": "1"
        }
      ],
      "default": "tff",
