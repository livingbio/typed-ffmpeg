{
  "description": "Deinterlace the input image.",
  "input_types": [
    [
      "default",
      "video"
    ]
  ],
  "is_dynamic_inputs": false,
  "is_dynamic_outputs": false,
  "is_slice_threading_supported": true,
  "is_support_framesync": false,
  "is_support_timeline": true,
  "name": "bwdif",
  "options": [
    {
      "alias": [
        "mode"
      ],
      "choices": [
        {
          "flags": "..FV.......",
          "help": "send one frame for each frame",
          "name": "send_frame",
          "value": "0"
        },
        {
          "flags": "..FV.......",
          "help": "send one frame for each field",
          "name": "send_field",
          "value": "1"
        }
      ],
      "default": "send_field",
      "description": "specify the interlacing mode (from 0 to 1) (default send_field)",
      "flags": "..FV.......",
      "max": "1",
      "min": "0",
      "name": "mode",
      "typing": "int"
    },
    {
      "alias": [
        "parity"
      ],
      "choices": [
        {
          "flags": "..FV.......",
          "help": "assume top field first",
          "name": "tff",
          "value": "0"
        },
        {
          "flags": "..FV.......",
          "help": "assume bottom field first",
          "name": "bff",
          "value": "1"
        },
        {
          "flags": "..FV.......",
          "help": "auto detect parity",
          "name": "auto",
          "value": "-1"
        }
      ],
      "default": "auto",
      "description": "specify the assumed picture field parity (from -1 to 1) (default auto)",
      "flags": "..FV.......",
      "max": "1",
      "min": "-1",
      "name": "parity",
      "typing": "int"
    },
    {
      "alias": [
        "deint"
      ],
      "choices": [
        {
          "flags": "..FV.......",
          "help": "deinterlace all frames",
          "name": "all",
          "value": "0"
        },
        {
          "flags": "..FV.......",
          "help": "only deinterlace frames marked as interlaced",
          "name": "interlaced",
          "value": "1"
        }
      ],
      "default": "all",
      "description": "specify which frames to deinterlace (from 0 to 1) (default all)",
      "flags": "..FV.......",
      "max": "1",
      "min": "0",
      "name": "deint",
      "typing": "int"
    }
  ],
  "output_types": [
    [
      "default",
      "video"
    ]
  ]
