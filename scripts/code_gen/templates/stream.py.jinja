from typing import Any
{% import "_components.jinja" as _components %}

class VideoStream:
    {% for f in filters %}
        {% if f.filter_type.value in ["vf", "vaf"] %}
    {{ _components.filter_method(f) }}
        {% endif %}
    {% endfor %}

class AudioStream:
    {% for f in filters %}
        {% if f.filter_type.value in ["af", "avf"] %}
    {{ _components.filter_method(f) }}
        {% endif %}
    {% endfor %}
