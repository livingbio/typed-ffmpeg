{
  "description": "Stack video inputs horizontally.",
  "filter_type": "vf",
  "formula_input_typings": "[StreamType.video] * int(inputs)",
  "formula_output_typings": null,
  "id": "ff_vf_hstack",
  "input_stream_typings": [],
  "is_input_dynamic": true,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "hstack",
  "options": [
    {
      "alias": [
        "inputs"
      ],
      "choices": [],
      "default": "2",
      "description": "set number of inputs (from 2 to INT_MAX) (default 2)",
      "name": "inputs",
      "required": false,
      "typing": "INT"
    },
    {
      "alias": [
        "shortest"
      ],
      "choices": [],
      "default": "false",
      "description": "force termination when the shortest input terminates (default false)",
      "name": "shortest",
      "required": false,
      "typing": "BOOLEAN"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#hstack"
}
