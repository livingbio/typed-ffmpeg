{
  "description": "Render text subtitles onto input video using the libass library.",
  "filter_type": "vf",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_vf_subtitles",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "subtitles",
  "options": [
    {
      "alias": [
        "filename",
        "f"
      ],
      "choices": [],
      "default": null,
      "description": "set the filename of file to read",
      "name": "filename",
      "required": false,
      "typing": "String"
    },
    {
      "alias": [
        "original_size"
      ],
      "choices": [],
      "default": null,
      "description": "set the size of the original video (used to scale fonts)",
      "name": "original_size",
      "required": false,
      "typing": "Image_size"
    },
    {
      "alias": [
        "fontsdir"
      ],
      "choices": [],
      "default": null,
      "description": "set the directory containing the fonts to read",
      "name": "fontsdir",
      "required": false,
      "typing": "String"
    },
    {
      "alias": [
        "alpha"
      ],
      "choices": [],
      "default": "false",
      "description": "enable processing of alpha channel (default false)",
      "name": "alpha",
      "required": false,
      "typing": "Boolean"
    },
    {
      "alias": [
        "charenc"
      ],
      "choices": [],
      "default": null,
      "description": "set input character encoding",
      "name": "charenc",
      "required": false,
      "typing": "String"
    },
    {
      "alias": [
        "stream_index",
        "si"
      ],
      "choices": [],
      "default": "-1",
      "description": "set stream index (from -1 to INT_MAX) (default -1)",
      "name": "stream_index",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "force_style"
      ],
      "choices": [],
      "default": null,
      "description": "force subtitle style",
      "name": "force_style",
      "required": false,
      "typing": "String"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#subtitles"
}
