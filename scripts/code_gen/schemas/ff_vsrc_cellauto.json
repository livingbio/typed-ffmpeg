{
  "description": "Create pattern generated by an elementary cellular automaton.",
  "filter_type": "vsrc",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_vsrc_cellauto",
  "input_stream_typings": [],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "cellauto",
  "options": [
    {
      "alias": [
        "filename",
        "f"
      ],
      "choices": [],
      "default": null,
      "description": "read initial pattern from file",
      "name": "filename",
      "required": false,
      "typing": "STRING"
    },
    {
      "alias": [
        "pattern",
        "p"
      ],
      "choices": [],
      "default": null,
      "description": "set initial pattern",
      "name": "pattern",
      "required": false,
      "typing": "STRING"
    },
    {
      "alias": [
        "rate",
        "r"
      ],
      "choices": [],
      "default": "25",
      "description": "set video rate (default \"25\")",
      "name": "rate",
      "required": false,
      "typing": "VIDEO_RATE"
    },
    {
      "alias": [
        "size",
        "s"
      ],
      "choices": [],
      "default": null,
      "description": "set video size",
      "name": "size",
      "required": false,
      "typing": "IMAGE_SIZE"
    },
    {
      "alias": [
        "rule"
      ],
      "choices": [],
      "default": "110",
      "description": "set rule (from 0 to 255) (default 110)",
      "name": "rule",
      "required": false,
      "typing": "INT"
    },
    {
      "alias": [
        "random_fill_ratio",
        "ratio"
      ],
      "choices": [],
      "default": "0.618034",
      "description": "set fill ratio for filling initial grid randomly (from 0 to 1) (default 0.618034)",
      "name": "random_fill_ratio",
      "required": false,
      "typing": "DOUBLE"
    },
    {
      "alias": [
        "random_seed",
        "seed"
      ],
      "choices": [],
      "default": "-1",
      "description": "set the seed for filling the initial grid randomly (from -1 to UINT32_MAX) (default -1)",
      "name": "random_seed",
      "required": false,
      "typing": "INT64"
    },
    {
      "alias": [
        "scroll"
      ],
      "choices": [],
      "default": "true",
      "description": "scroll pattern downward (default true)",
      "name": "scroll",
      "required": false,
      "typing": "BOOLEAN"
    },
    {
      "alias": [
        "start_full"
      ],
      "choices": [],
      "default": "false",
      "description": "start filling the whole video (default false)",
      "name": "start_full",
      "required": false,
      "typing": "BOOLEAN"
    },
    {
      "alias": [
        "full"
      ],
      "choices": [],
      "default": "true",
      "description": "start filling the whole video (default true)",
      "name": "full",
      "required": false,
      "typing": "BOOLEAN"
    },
    {
      "alias": [
        "stitch"
      ],
      "choices": [],
      "default": "true",
      "description": "stitch boundaries (default true)",
      "name": "stitch",
      "required": false,
      "typing": "BOOLEAN"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#cellauto"
}
