{
  "description": "18.1 a3dscope\nConvert input audio to 3d scope video output.\n\nThe filter accepts the following options:",
  "filter_type": "avf",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_avf_a3dscope",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_AUDIO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "name": "a3dscope",
  "options": [
    {
      "alias": [
        "rate",
        "r"
      ],
      "default": "25",
      "description": "<dd><p>Set frame rate, expressed as number of frames per second. Default\nvalue is \"25\".\n</p>\n</dd>",
      "name": "rate",
      "required": false,
      "typing": "str"
    },
    {
      "alias": [
        "size",
        "s"
      ],
      "default": "hd720",
      "description": "<dd><p>Specify the video size for the output. For the syntax of this option, check the\n<a href=\"ffmpeg-utils.html#video-size-syntax\">(ffmpeg-utils)\"Video size\" section in the ffmpeg-utils manual</a>.\nDefault value is <code>hd720</code>.\n</p>\n</dd>",
      "name": "size",
      "required": false,
      "typing": "str"
    },
    {
      "alias": [
        "fov"
      ],
      "default": "90.f",
      "description": "<dd><p>Set the camera field of view. Default is 90 degrees.\nAllowed range is from 40 to 150.\n</p>\n</dd>",
      "name": "fov",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "roll"
      ],
      "default": "0.f",
      "description": "<dd><p>Set the camera roll.\n</p>\n</dd>",
      "name": "roll",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "pitch"
      ],
      "default": "0.f",
      "description": "<dd><p>Set the camera pitch.\n</p>\n</dd>",
      "name": "pitch",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "yaw"
      ],
      "default": "0.f",
      "description": "<dd><p>Set the camera yaw.\n</p>\n</dd>",
      "name": "yaw",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "xzoom"
      ],
      "default": "1.f",
      "description": "<dd><p>Set the camera zoom on X-axis.\n</p>\n</dd>",
      "name": "xzoom",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "yzoom"
      ],
      "default": "1.f",
      "description": "<dd><p>Set the camera zoom on Y-axis.\n</p>\n</dd>",
      "name": "yzoom",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "zzoom"
      ],
      "default": "1.f",
      "description": "<dd><p>Set the camera zoom on Z-axis.\n</p>\n</dd>",
      "name": "zzoom",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "xpos"
      ],
      "default": "0.f",
      "description": "<dd><p>Set the camera position on X-axis.\n</p>\n</dd>",
      "name": "xpos",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "ypos"
      ],
      "default": "0.f",
      "description": "<dd><p>Set the camera position on Y-axis.\n</p>\n</dd>",
      "name": "ypos",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "zpos"
      ],
      "default": "0.f",
      "description": "<dd><p>Set the camera position on Z-axis.\n</p>\n</dd>",
      "name": "zpos",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "length"
      ],
      "default": 15,
      "description": "<dd><p>Set the length of displayed audio waves in number of frames.\n</p></dd>",
      "name": "length",
      "required": false,
      "typing": "int"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#a3dscope"
}
