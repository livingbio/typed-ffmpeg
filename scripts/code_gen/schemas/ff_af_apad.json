{
  "description": "Pad audio with silence.",
  "filter_type": "af",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_af_apad",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "audio"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": true,
  "name": "apad",
  "options": [
    {
      "alias": [
        "packet_size"
      ],
      "choices": [],
      "default": "4096",
      "description": "set silence packet size (from 0 to INT_MAX) (default 4096)",
      "name": "packet_size",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "pad_len"
      ],
      "choices": [],
      "default": "-1",
      "description": "set number of samples of silence to add (from -1 to I64_MAX) (default -1)",
      "name": "pad_len",
      "required": false,
      "typing": "Int64"
    },
    {
      "alias": [
        "whole_len"
      ],
      "choices": [],
      "default": "-1",
      "description": "set minimum target number of samples in the audio stream (from -1 to I64_MAX) (default -1)",
      "name": "whole_len",
      "required": false,
      "typing": "Int64"
    },
    {
      "alias": [
        "pad_dur"
      ],
      "choices": [],
      "default": "-0.000001",
      "description": "set duration of silence to add (default -0.000001)",
      "name": "pad_dur",
      "required": false,
      "typing": "Duration"
    },
    {
      "alias": [
        "whole_dur"
      ],
      "choices": [],
      "default": "-0.000001",
      "description": "set minimum target duration in the audio stream (default -0.000001)",
      "name": "whole_dur",
      "required": false,
      "typing": "Duration"
    },
    {
      "alias": [],
      "choices": [],
      "default": null,
      "description": "timeline editing",
      "name": "enable",
      "required": false,
      "typing": "str"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "audio"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#apad"
}
