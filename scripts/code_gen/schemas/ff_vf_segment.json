{
  "description": "Segment video stream.",
  "filter_type": "vf",
  "formula_input_typings": null,
  "formula_output_typings": "[StreamType.video] * len((str(timestamps or frames)).split('|'))",
  "id": "ff_vf_segment",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": true,
  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "segment",
  "options": [
    {
      "alias": [
        "timestamps"
      ],
      "choices": [],
      "default": "((void*)0)",
      "description": "<dd><p>Timestamps of output segments separated by \u2019|\u2019. The first segment will run\nfrom the beginning of the input stream. The last segment will run until\nthe end of the input stream\n</p>\n</dd>",
      "name": "timestamps",
      "required": false,
      "typing": "str"
    },
    {
      "alias": [
        "frames"
      ],
      "choices": [],
      "default": "((void*)0)",
      "description": "<dd><p>Exact frame/sample count to split the segments.\n</p></dd>",
      "name": "frames",
      "required": false,
      "typing": "str"
    }
  ],
  "output_stream_typings": [],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#segment_002c-asegment"
}
