{
  "description": "Apply custom Color Maps to video stream.",
  "filter_type": "vf",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_vf_colormap",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "video"
    },
    {
      "name": "source",
      "type": "video"
    },
    {
      "name": "target",
      "type": "video"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": true,
  "name": "colormap",
  "options": [
    {
      "alias": [
        "patch_size"
      ],
      "choices": [],
      "default": "\"64x64\"",
      "description": "set patch size (default \"64x64\")",
      "name": "patch_size",
      "required": false,
      "typing": "Image_size"
    },
    {
      "alias": [
        "nb_patches"
      ],
      "choices": [],
      "default": 0,
      "description": "set number of patches (from 0 to 64) (default 0)",
      "name": "nb_patches",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "type"
      ],
      "choices": [
        {
          "help": "the target colors are relative",
          "name": "relative",
          "value": "0"
        },
        {
          "help": "the target colors are absolute",
          "name": "absolute",
          "value": "1"
        }
      ],
      "default": "absolute",
      "description": "set the target type used (from 0 to 1) (default absolute)",
      "name": "type",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "kernel"
      ],
      "choices": [
        {
          "help": "square root of sum of squared differences",
          "name": "euclidean",
          "value": "0"
        },
        {
          "help": "weighted square root of sum of squared differences",
          "name": "weuclidean",
          "value": "1"
        }
      ],
      "default": "euclidean",
      "description": "set the kernel used for measuring color difference (from 0 to 1) (default euclidean)",
      "name": "kernel",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [],
      "choices": [],
      "default": null,
      "description": "timeline editing",
      "name": "enable",
      "required": false,
      "typing": "str"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "video"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#colormap"
}
