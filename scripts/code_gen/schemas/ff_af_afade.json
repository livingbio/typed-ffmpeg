{
  "description": "Fade in/out input audio.",
  "filter_type": "af",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_af_afade",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_AUDIO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": true,
  "name": "afade",
  "options": [
    {
      "alias": [
        "type",
        "t"
      ],
      "choices": [
        {
          "help": "fade-in",
          "name": "in",
          "value": 0
        },
        {
          "help": "fade-out",
          "name": "out",
          "value": 1
        }
      ],
      "default": 0,
      "description": "<dd><p>Specify the effect type, can be either <code>in</code> for fade-in, or\n<code>out</code> for a fade-out effect. Default is <code>in</code>.\n</p>\n</dd>",
      "name": "type",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "start_sample",
        "ss"
      ],
      "choices": [],
      "default": 0,
      "description": "<dd><p>Specify the number of the start sample for starting to apply the fade\neffect. Default is 0.\n</p>\n</dd>",
      "name": "start_sample",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "nb_samples",
        "ns"
      ],
      "choices": [],
      "default": 44100,
      "description": "<dd><p>Specify the number of samples for which the fade effect has to last. At\nthe end of the fade-in effect the output audio will have the same\nvolume as the input audio, at the end of the fade-out transition\nthe output audio will be silence. Default is 44100.\n</p>\n</dd>",
      "name": "nb_samples",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "start_time",
        "st"
      ],
      "choices": [],
      "default": 0,
      "description": "<dd><p>Specify the start time of the fade effect. Default is 0.\nThe value must be specified as a time duration; see\n<a href=\"ffmpeg-utils.html#time-duration-syntax\">(ffmpeg-utils)the Time duration section in the ffmpeg-utils(1) manual</a>\nfor the accepted syntax.\nIf set this option is used instead of <var>start_sample</var>.\n</p>\n</dd>",
      "name": "start_time",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "duration",
        "d"
      ],
      "choices": [],
      "default": 0,
      "description": "<dd><p>Specify the duration of the fade effect. See\n<a href=\"ffmpeg-utils.html#time-duration-syntax\">(ffmpeg-utils)the Time duration section in the ffmpeg-utils(1) manual</a>\nfor the accepted syntax.\nAt the end of the fade-in effect the output audio will have the same\nvolume as the input audio, at the end of the fade-out transition\nthe output audio will be silence.\nBy default the duration is determined by <var>nb_samples</var>.\nIf set this option is used instead of <var>nb_samples</var>.\n</p>\n</dd>",
      "name": "duration",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "curve",
        "c"
      ],
      "choices": [
        {
          "help": "no fade; keep audio as-is",
          "name": "nofade",
          "value": "NONE"
        },
        {
          "help": "linear slope",
          "name": "tri",
          "value": "TRI"
        },
        {
          "help": "quarter of sine wave",
          "name": "qsin",
          "value": "QSIN"
        },
        {
          "help": "exponential sine wave",
          "name": "esin",
          "value": "ESIN"
        },
        {
          "help": "half of sine wave",
          "name": "hsin",
          "value": "HSIN"
        },
        {
          "help": "logarithmic",
          "name": "log",
          "value": "LOG"
        },
        {
          "help": "inverted parabola",
          "name": "ipar",
          "value": "IPAR"
        },
        {
          "help": "quadratic",
          "name": "qua",
          "value": "QUA"
        },
        {
          "help": "cubic",
          "name": "cub",
          "value": "CUB"
        },
        {
          "help": "square root",
          "name": "squ",
          "value": "SQU"
        },
        {
          "help": "cubic root",
          "name": "cbr",
          "value": "CBR"
        },
        {
          "help": "parabola",
          "name": "par",
          "value": "PAR"
        },
        {
          "help": "exponential",
          "name": "exp",
          "value": "EXP"
        },
        {
          "help": "inverted quarter of sine wave",
          "name": "iqsin",
          "value": "IQSIN"
        },
        {
          "help": "inverted half of sine wave",
          "name": "ihsin",
          "value": "IHSIN"
        },
        {
          "help": "double-exponential seat",
          "name": "dese",
          "value": "DESE"
        },
        {
          "help": "double-exponential sigmoid",
          "name": "desi",
          "value": "DESI"
        },
        {
          "help": "logistic sigmoid",
          "name": "losi",
          "value": "LOSI"
        },
        {
          "help": "sine cardinal function",
          "name": "sinc",
          "value": "SINC"
        },
        {
          "help": "inverted sine cardinal function",
          "name": "isinc",
          "value": "ISINC"
        },
        {
          "help": "quartic",
          "name": "quat",
          "value": "QUAT"
        },
        {
          "help": "quartic root",
          "name": "quatr",
          "value": "QUATR"
        },
        {
          "help": "squared quarter of sine wave",
          "name": "qsin2",
          "value": "QSIN2"
        },
        {
          "help": "squared half of sine wave",
          "name": "hsin2",
          "value": "HSIN2"
        }
      ],
      "default": "TRI",
      "description": "<dd><p>Set curve for fade transition.\n</p>\n<p>It accepts the following values:\n</p><dl compact=\"compact\">\n<dt><samp>tri</samp></dt>\n<dd><p>select triangular, linear slope (default)\n</p></dd>\n<dt><samp>qsin</samp></dt>\n<dd><p>select quarter of sine wave\n</p></dd>\n<dt><samp>hsin</samp></dt>\n<dd><p>select half of sine wave\n</p></dd>\n<dt><samp>esin</samp></dt>\n<dd><p>select exponential sine wave\n</p></dd>\n<dt><samp>log</samp></dt>\n<dd><p>select logarithmic\n</p></dd>\n<dt><samp>ipar</samp></dt>\n<dd><p>select inverted parabola\n</p></dd>\n<dt><samp>qua</samp></dt>\n<dd><p>select quadratic\n</p></dd>\n<dt><samp>cub</samp></dt>\n<dd><p>select cubic\n</p></dd>\n<dt><samp>squ</samp></dt>\n<dd><p>select square root\n</p></dd>\n<dt><samp>cbr</samp></dt>\n<dd><p>select cubic root\n</p></dd>\n<dt><samp>par</samp></dt>\n<dd><p>select parabola\n</p></dd>\n<dt><samp>exp</samp></dt>\n<dd><p>select exponential\n</p></dd>\n<dt><samp>iqsin</samp></dt>\n<dd><p>select inverted quarter of sine wave\n</p></dd>\n<dt><samp>ihsin</samp></dt>\n<dd><p>select inverted half of sine wave\n</p></dd>\n<dt><samp>dese</samp></dt>\n<dd><p>select double-exponential seat\n</p></dd>\n<dt><samp>desi</samp></dt>\n<dd><p>select double-exponential sigmoid\n</p></dd>\n<dt><samp>losi</samp></dt>\n<dd><p>select logistic sigmoid\n</p></dd>\n<dt><samp>sinc</samp></dt>\n<dd><p>select sine cardinal function\n</p></dd>\n<dt><samp>isinc</samp></dt>\n<dd><p>select inverted sine cardinal function\n</p></dd>\n<dt><samp>quat</samp></dt>\n<dd><p>select quartic\n</p></dd>\n<dt><samp>quatr</samp></dt>\n<dd><p>select quartic root\n</p></dd>\n<dt><samp>qsin2</samp></dt>\n<dd><p>select squared quarter of sine wave\n</p></dd>\n<dt><samp>hsin2</samp></dt>\n<dd><p>select squared half of sine wave\n</p></dd>\n<dt><samp>nofade</samp></dt>\n<dd><p>no fade applied\n</p></dd>\n</dl>\n</dd>",
      "name": "curve",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "silence"
      ],
      "choices": [],
      "default": 0.0,
      "description": "<dd><p>Set the initial gain for fade-in or final gain for fade-out.\nDefault value is <code>0.0</code>.\n</p>\n</dd>",
      "name": "silence",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "unity"
      ],
      "choices": [],
      "default": 1.0,
      "description": "<dd><p>Set the initial gain for fade-out or final gain for fade-in.\nDefault value is <code>1.0</code>.\n</p></dd>",
      "name": "unity",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [],
      "choices": [],
      "default": null,
      "description": "timeline editing",
      "name": "enable",
      "required": false,
      "typing": "str"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_AUDIO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#afade"
}
