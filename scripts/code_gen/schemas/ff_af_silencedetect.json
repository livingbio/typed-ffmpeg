{
  "description": "Detect silence.",
  "filter_type": "af",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_af_silencedetect",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_AUDIO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "silencedetect",
  "options": [
    {
      "alias": [
        "n",
        "noise"
      ],
      "choices": [],
      "default": 0.001,
      "description": "<dd><p>Set noise tolerance. Can be specified in dB (in case \"dB\" is appended to the\nspecified value) or amplitude ratio. Default is -60dB, or 0.001.\n</p>\n</dd>",
      "name": "n",
      "required": false,
      "typing": "float"
    },
    {
      "alias": [
        "d",
        "duration"
      ],
      "choices": [],
      "default": 2000000,
      "description": "<dd><p>Set silence duration until notification (default is 2 seconds). See\n<a href=\"ffmpeg-utils.html#time-duration-syntax\">(ffmpeg-utils)the Time duration section in the ffmpeg-utils(1) manual</a>\nfor the accepted syntax.\n</p>\n</dd>",
      "name": "d",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "mono",
        "m"
      ],
      "choices": [],
      "default": 0,
      "description": "<dd><p>Process each channel separately, instead of combined. By default is disabled.\n</p></dd>",
      "name": "mono",
      "required": false,
      "typing": "bool"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_AUDIO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#silencedetect"
}
