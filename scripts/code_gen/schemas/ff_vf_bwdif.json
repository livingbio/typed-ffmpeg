{
  "description": "Deinterlace the input image.",
  "filter_type": "vf",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_vf_bwdif",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": true,
  "name": "bwdif",
  "options": [
    {
      "alias": [
        "mode"
      ],
      "choices": [
        {
          "help": "send one frame for each frame",
          "name": "send_frame",
          "value": "0"
        },
        {
          "help": "send one frame for each field",
          "name": "send_field",
          "value": "1"
        }
      ],
      "default": "send_field",
      "description": "specify the interlacing mode (from 0 to 1) (default send_field)",
      "name": "mode",
      "required": false,
      "typing": "INT"
    },
    {
      "alias": [
        "parity"
      ],
      "choices": [
        {
          "help": "assume top field first",
          "name": "tff",
          "value": "0"
        },
        {
          "help": "assume bottom field first",
          "name": "bff",
          "value": "1"
        },
        {
          "help": "auto detect parity",
          "name": "auto",
          "value": "-1"
        }
      ],
      "default": "auto",
      "description": "specify the assumed picture field parity (from -1 to 1) (default auto)",
      "name": "parity",
      "required": false,
      "typing": "INT"
    },
    {
      "alias": [
        "deint"
      ],
      "choices": [
        {
          "help": "deinterlace all frames",
          "name": "all",
          "value": "0"
        },
        {
          "help": "only deinterlace frames marked as interlaced",
          "name": "interlaced",
          "value": "1"
        }
      ],
      "default": "all",
      "description": "specify which frames to deinterlace (from 0 to 1) (default all)",
      "name": "deint",
      "required": false,
      "typing": "INT"
    },
    {
      "alias": [],
      "choices": [],
      "default": null,
      "description": "timeline editing",
      "name": "enable",
      "required": false,
      "typing": "str"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#bwdif"
}
