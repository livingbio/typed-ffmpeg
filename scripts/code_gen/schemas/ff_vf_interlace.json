{
  "description": "Convert progressive video into interlaced.",
  "filter_type": "vf",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_vf_interlace",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": false,
  "name": "interlace",
  "options": [
    {
      "alias": [
        "scan"
      ],
      "choices": [],
      "default": "MODE_TFF",
      "description": "<dd><p>This determines whether the interlaced frame is taken from the even\n(tff - default) or odd (bff) lines of the progressive frame.\n</p>\n</dd>",
      "name": "scan",
      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "lowpass"
      ],
      "choices": [
        {
          "help": "disable vertical low-pass filter",
          "name": "off",
          "value": "VLPF_OFF"
        },
        {
          "help": "linear vertical low-pass filter",
          "name": "linear",
          "value": "VLPF_LIN"
        },
        {
          "help": "complex vertical low-pass filter",
          "name": "complex",
          "value": "VLPF_CMP"
        }
      ],
      "default": "VLPF_LIN",
      "description": "<dd><p>Vertical lowpass filter to avoid twitter interlacing and\nreduce moire patterns.\n</p>\n<dl compact=\"compact\">\n<dt>\u2018<samp>0, off</samp>\u2019</dt>\n<dd><p>Disable vertical lowpass filter\n</p>\n</dd>\n<dt>\u2018<samp>1, linear</samp>\u2019</dt>\n<dd><p>Enable linear filter (default)\n</p>\n</dd>\n<dt>\u2018<samp>2, complex</samp>\u2019</dt>\n<dd><p>Enable complex filter. This will slightly less reduce twitter and moire\nbut better retain detail and subjective sharpness impression.\n</p>\n</dd>\n</dl>\n</dd>",
      "name": "lowpass",
      "required": false,
      "typing": "int"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "AVMEDIA_TYPE_VIDEO"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#interlace"
}
