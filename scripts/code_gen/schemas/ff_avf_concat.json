      "required": false,
      "typing": "int"
    },
    {
      "alias": [
        "unsafe"
      ],
      "choices": [],
      "default": 0,
      "description": "<dd><p>Activate unsafe mode: do not fail if segments have a different format.\n</p>\n</dd>",
      "name": "unsafe",
      "required": false,
      "typing": "bool"
    }
  ],
  "output_stream_typings": [],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#concat"
