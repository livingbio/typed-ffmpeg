{
  "description": "Apply Edge Slope Tracing deinterlace.",
  "filter_type": "vf",
  "formula_input_typings": null,
  "formula_output_typings": null,
  "id": "ff_vf_estdif",
  "input_stream_typings": [
    {
      "name": "default",
      "type": "video"
    }
  ],
  "is_input_dynamic": false,
  "is_output_dynamic": false,
  "is_support_framesync": false,
  "is_support_timeline": true,
  "name": "estdif",
  "options": [
    {
      "alias": [
        "mode"
      ],
      "choices": [
        {
          "help": "send one frame for each frame",
          "name": "frame",
          "value": "0"
        },
        {
          "help": "send one frame for each field",
          "name": "field",
          "value": "1"
        }
      ],
      "default": "field",
      "description": "specify the mode (from 0 to 1) (default field)",
      "name": "mode",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "parity"
      ],
      "choices": [
        {
          "help": "assume top field first",
          "name": "tff",
          "value": "0"
        },
        {
          "help": "assume bottom field first",
          "name": "bff",
          "value": "1"
        },
        {
          "help": "auto detect parity",
          "name": "auto",
          "value": "-1"
        }
      ],
      "default": "auto",
      "description": "specify the assumed picture field parity (from -1 to 1) (default auto)",
      "name": "parity",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "deint"
      ],
      "choices": [
        {
          "help": "deinterlace all frames",
          "name": "all",
          "value": "0"
        },
        {
          "help": "only deinterlace frames marked as interlaced",
          "name": "interlaced",
          "value": "1"
        }
      ],
      "default": "all",
      "description": "specify which frames to deinterlace (from 0 to 1) (default all)",
      "name": "deint",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "rslope"
      ],
      "choices": [],
      "default": 1,
      "description": "specify the search radius for edge slope tracing (from 1 to 15) (default 1)",
      "name": "rslope",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "redge"
      ],
      "choices": [],
      "default": 2,
      "description": "specify the search radius for best edge matching (from 0 to 15) (default 2)",
      "name": "redge",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [
        "ecost"
      ],
      "choices": [],
      "default": 1.0,
      "description": "specify the edge cost for edge matching (from 0 to 9) (default 1)",
      "name": "ecost",
      "required": false,
      "typing": "Float"
    },
    {
      "alias": [
        "mcost"
      ],
      "choices": [],
      "default": 0.5,
      "description": "specify the middle cost for edge matching (from 0 to 1) (default 0.5)",
      "name": "mcost",
      "required": false,
      "typing": "Float"
    },
    {
      "alias": [
        "dcost"
      ],
      "choices": [],
      "default": 0.5,
      "description": "specify the distance cost for edge matching (from 0 to 1) (default 0.5)",
      "name": "dcost",
      "required": false,
      "typing": "Float"
    },
    {
      "alias": [
        "interp"
      ],
      "choices": [
        {
          "help": "two-point interpolation",
          "name": "2p",
          "value": "0"
        },
        {
          "help": "four-point interpolation",
          "name": "4p",
          "value": "1"
        },
        {
          "help": "six-point interpolation",
          "name": "6p",
          "value": "2"
        }
      ],
      "default": "4p",
      "description": "specify the type of interpolation (from 0 to 2) (default 4p)",
      "name": "interp",
      "required": false,
      "typing": "Int"
    },
    {
      "alias": [],
      "choices": [],
      "default": null,
      "description": "timeline editing",
      "name": "enable",
      "required": false,
      "typing": "str"
    }
  ],
  "output_stream_typings": [
    {
      "name": "default",
      "type": "video"
    }
  ],
  "ref": "https://ffmpeg.org/ffmpeg-filters.html#estdif"
}
